<div class="form">
<%= simple_form_for(@creation) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
<%= f.input :images, as: :file, input_html: { multiple: true } %>
<%= f.submit class: 'btn btn-primary' %>
<% end %>
</div>
